{
    "manifest_version": 3,
    "name": "The Good Place Word Replacer",
    "version": "1.0",
    "description": "Replaces bad words with good words like in The Good Place.",
    "icons": {
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }
  